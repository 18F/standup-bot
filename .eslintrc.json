{
  "extends": "airbnb-base",
  "rules": {
    "class-methods-use-this": 0,
    "comma-dangle": [
      1,
      "never"
    ],
    "max-len": 0,
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "prefer-destructuring": [
      "error",
      {
        "array": false,
        "object": true
      }
    ]
  },
  "env": {
    "node": true
  }
}
